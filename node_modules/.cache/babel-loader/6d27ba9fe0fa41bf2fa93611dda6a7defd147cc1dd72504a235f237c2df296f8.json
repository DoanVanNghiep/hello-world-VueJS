{"ast":null,"code":"import EmployeeService from '../service/EmployeeService';\nexport default {\n  data() {\n    return {\n      id: this.$route.params.id,\n      employee: {\n        empNo: '',\n        empName: '',\n        position: '',\n        photoUrl: null,\n        quantity: 0,\n        price: 0.0\n      }\n    };\n  },\n  mounted() {\n    this.fetchEmployee();\n  },\n  methods: {\n    fetchEmployee() {\n      EmployeeService.getEmployeeById(this.id).then(res => {\n        this.employee = res.data;\n      }).catch(error => {\n        console.error('Error fetching employee:', error);\n        alert('Could not retrieve employee data. Please try again later.');\n      });\n    },\n    updateEmployee() {\n      EmployeeService.updateEmployee(this.id, this.employee).then(() => {\n        this.$router.push('/employee');\n      }).catch(error => {\n        console.error('Error updating employee:', error);\n        alert('An error occurred while updating the employee. Please try again.');\n      });\n    },\n    changePhotoHandler(event) {\n      const file = event.target.files[0];\n      const formData = new FormData();\n      formData.append('file', file);\n      EmployeeService.uploadPhoto(formData).then(response => {\n        this.employee.photoUrl = response.data;\n      }).catch(error => {\n        console.error('Error uploading photo:', error);\n        alert('An error occurred while uploading the photo. Please try again.');\n      });\n    },\n    cancel() {\n      this.$router.push('/employee');\n    }\n  }\n};","map":{"version":3,"names":["EmployeeService","data","id","$route","params","employee","empNo","empName","position","photoUrl","quantity","price","mounted","fetchEmployee","methods","getEmployeeById","then","res","catch","error","console","alert","updateEmployee","$router","push","changePhotoHandler","event","file","target","files","formData","FormData","append","uploadPhoto","response","cancel"],"sources":["E:\\VueJS\\hello-world\\src\\components\\UpdateEmployeeComponent.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <br />\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"card col-md-6 offset-md-3\">\r\n            <h3 class=\"text-center\">Update Employee</h3>\r\n            <div class=\"card-body\">\r\n              <form @submit.prevent=\"updateEmployee\">\r\n                <div class=\"form-group\">\r\n                  <label>Employee No:</label>\r\n                  <input\r\n                    placeholder=\"Employee No\"\r\n                    name=\"empNo\"\r\n                    class=\"form-control\"\r\n                    v-model=\"employee.empNo\"\r\n                    readonly\r\n                  />\r\n                </div>\r\n  \r\n                <div class=\"form-group\">\r\n                  <label>Employee Name:</label>\r\n                  <input\r\n                    placeholder=\"Employee Name\"\r\n                    name=\"empName\"\r\n                    class=\"form-control\"\r\n                    v-model=\"employee.empName\"\r\n                  />\r\n                </div>\r\n  \r\n                <div class=\"form-group\">\r\n                  <label>Position:</label>\r\n                  <input\r\n                    placeholder=\"Position\"\r\n                    name=\"position\"\r\n                    class=\"form-control\"\r\n                    v-model=\"employee.position\"\r\n                  />\r\n                </div>\r\n  \r\n                <div class=\"form-group\">\r\n                  <label>Quantity:</label>\r\n                  <input\r\n                    placeholder=\"Quantity\"\r\n                    name=\"quantity\"\r\n                    type=\"number\"\r\n                    class=\"form-control\"\r\n                    v-model=\"employee.quantity\"\r\n                  />\r\n                </div>\r\n  \r\n                <div class=\"form-group\">\r\n                  <label>Price:</label>\r\n                  <input\r\n                    placeholder=\"Price\"\r\n                    name=\"price\"\r\n                    type=\"number\"\r\n                    step=\"0.01\"\r\n                    class=\"form-control\"\r\n                    v-model=\"employee.price\"\r\n                  />\r\n                </div>\r\n  \r\n                <div class=\"form-group\">\r\n                  <label>Photo:</label>\r\n                  <input type=\"file\" name=\"photo\" class=\"form-control\" @change=\"changePhotoHandler\" />\r\n                </div>\r\n  \r\n                <div v-if=\"employee.photoUrl\" class=\"form-group\">\r\n                  <img :src=\"employee.photoUrl\" :alt=\"employee.empName\" style=\"width: 100px; height: 100px\" />\r\n                </div>\r\n  \r\n                <button class=\"btn btn-success\" type=\"submit\">Update</button>\r\n                <button class=\"btn btn-danger\" @click=\"cancel\" style=\"margin-left: 10px\">Cancel</button>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import EmployeeService from '../service/EmployeeService';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        id: this.$route.params.id,\r\n        employee: {\r\n          empNo: '',\r\n          empName: '',\r\n          position: '',\r\n          photoUrl: null,\r\n          quantity: 0,\r\n          price: 0.0\r\n        }\r\n      };\r\n    },\r\n    mounted() {\r\n      this.fetchEmployee();\r\n    },\r\n    methods: {\r\n      fetchEmployee() {\r\n        EmployeeService.getEmployeeById(this.id)\r\n          .then(res => {\r\n            this.employee = res.data;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching employee:', error);\r\n            alert('Could not retrieve employee data. Please try again later.');\r\n          });\r\n      },\r\n      updateEmployee() {\r\n        EmployeeService.updateEmployee(this.id, this.employee)\r\n          .then(() => {\r\n            this.$router.push('/employee');\r\n          })\r\n          .catch(error => {\r\n            console.error('Error updating employee:', error);\r\n            alert('An error occurred while updating the employee. Please try again.');\r\n          });\r\n      },\r\n      changePhotoHandler(event) {\r\n        const file = event.target.files[0];\r\n        const formData = new FormData();\r\n        formData.append('file', file);\r\n  \r\n        EmployeeService.uploadPhoto(formData)\r\n          .then(response => {\r\n            this.employee.photoUrl = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error uploading photo:', error);\r\n            alert('An error occurred while uploading the photo. Please try again.');\r\n          });\r\n      },\r\n      cancel() {\r\n        this.$router.push('/employee');\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .card {\r\n    margin-top: 20px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAmFE,OAAOA,eAAc,MAAO,4BAA4B;AAExD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,EAAE,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,EAAE;MACzBG,QAAQ,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,IAAI;QACdC,QAAQ,EAAE,CAAC;QACXC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACPD,aAAaA,CAAA,EAAG;MACdb,eAAe,CAACe,eAAe,CAAC,IAAI,CAACb,EAAE,EACpCc,IAAI,CAACC,GAAE,IAAK;QACX,IAAI,CAACZ,QAAO,GAAIY,GAAG,CAAChB,IAAI;MAC1B,CAAC,EACAiB,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;QAChDE,KAAK,CAAC,2DAA2D,CAAC;MACpE,CAAC,CAAC;IACN,CAAC;IACDC,cAAcA,CAAA,EAAG;MACftB,eAAe,CAACsB,cAAc,CAAC,IAAI,CAACpB,EAAE,EAAE,IAAI,CAACG,QAAQ,EAClDW,IAAI,CAAC,MAAM;QACV,IAAI,CAACO,OAAO,CAACC,IAAI,CAAC,WAAW,CAAC;MAChC,CAAC,EACAN,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA