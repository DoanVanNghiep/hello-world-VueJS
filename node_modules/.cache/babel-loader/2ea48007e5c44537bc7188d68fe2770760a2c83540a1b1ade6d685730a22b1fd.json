           <div class=\"form-group\">\r\n                  <label>Price:</label>\r\n                  <input\r\n                    placeholder=\"Price\"\r\n                    type=\"number\"\r\n                    step=\"0.01\"\r\n                    v-model=\"price\"\r\n                    class=\"form-control\"\r\n                  />\r\n                </div>\r\n                <div class=\"form-group\">\r\n                  <label>Photo:</label>\r\n                  <input type=\"file\" class=\"form-control\" @change=\"changePhotoHandler\" />\r\n                </div>\r\n                <div v-if=\"photoUrl\" class=\"form-group\">\r\n                  <img :src=\"photoUrl\" alt=\"Employee Photo\" style=\"width: 100px; height: 100px;\" />\r\n                </div>\r\n                <button class=\"btn btn-success\" @click=\"saveOrUpdateEmployee\">Save</button>\r\n                <button class=\"btn btn-danger\" @click=\"cancel\" style=\"margin-left: 10px;\">Cancel</button>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import EmployeeService from '../service/EmployeeService';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        id: this.$route.params.id || '_add',\r\n        empNo: '',\r\n        empName: '',\r\n        position: '',\r\n        photoUrl: '',\r\n        quantity: 0,\r\n        price: 0.0\r\n      };\r\n    },\r\n    mounted() {\r\n      if (this.id !== '_add') {\r\n        EmployeeService.getEmployeeById(this.id).then((res) => {\r\n          const employee = res.data;\r\n          this.empNo = employee.empNo;\r\n          this.empName = employee.empName;\r\n          this.position = employee.position;\r\n          this.photoUrl = employee.photoUrl;\r\n          this.quantity = employee.quantity;\r\n          this.price = employee.price;\r\n        });\r\n      }\r\n    },\r\n    methods: {\r\n      saveOrUpdateEmployee() {\r\n        const employee = {\r\n          empNo: this.empNo,\r\n          empName: this.empName,\r\n          position: this.position,\r\n          photoUrl: this.photoUrl,\r\n          quantity: this.quantity,\r\n          price: this.price\r\n        };\r\n  \r\n        if (this.id === '_add') {\r\n          EmployeeService.createEmployee(employee).then(() => {\r\n            this.$router.push('/employee');\r\n          });\r\n        } else {\r\n          EmployeeService.updateEmployee(this.id, employee).then(() => {\r\n            this.$router.push('/employee');\r\n          });\r\n        }\r\n      },\r\n      changePhotoHandler(event) {\r\n        const file = event.target.files[0];\r\n        const formData = new FormData();\r\n        formData.append('file', file);\r\n  \r\n        EmployeeService.uploadPhoto(formData)\r\n          .then((response) => {\r\n            this.photoUrl = response.data;\r\n          })\r\n          .catch((error) => {\r\n            console.error('Error uploading photo:', error);\r\n            alert('An error occurred while uploading the photo. Please try again.');\r\n          });\r\n      },\r\n      cancel() {\r\n        this.$router.push('/employee');\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .container {\r\n    margin-top: 20px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAsEE,OAAOA,eAAc,MAAO,4BAA4B;AAExD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,EAAE,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,EAAC,IAAK,MAAM;MACnCG,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,CAAC;MACXC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,IAAI,CAACT,EAAC,KAAM,MAAM,EAAE;MACtBF,eAAe,CAACY,eAAe,CAAC,IAAI,CAACV,EAAE,CAAC,CAACW,IAAI,CAAEC,GAAG,IAAK;QACrD,MAAMC,QAAO,GAAID,GAAG,CAACb,IAAI;QACzB,IAAI,CAACI,KAAI,GAAIU,QAAQ,CAACV,KAAK;QAC3B,IAAI,CAACC,OAAM,GAAIS,QAAQ,CAACT,OAAO;QAC/B,IAAI,CAACC,QAAO,GAAIQ,QAAQ,CAACR,QAAQ;QACjC,IAAI,CAACC,QAAO,GAAIO,QAAQ,CAACP